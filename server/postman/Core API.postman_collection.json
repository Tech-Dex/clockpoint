								"exec": [
									"responseBody = pm.response.json()",
									"pm.environment.set(\"accessToken\", responseBody[\"user\"][\"token\"]);",
									"pm.environment.set(\"isActive\", responseBody[\"user\"][\"isActive\"]);",
